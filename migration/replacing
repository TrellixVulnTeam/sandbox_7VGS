1 replace peak for lax in gnmedia.net.hosts

sed 's/.si.stg.peak/.si.stg.lax/g' gnmedia.net.hosts
double check 
sed 's/.si.stg.peak/.si.stg.lax/g' gnmedia.net.hosts | grep '\.si'| grep 'stg'

2 replace lax for old
sed 's/.si.stg.lax/.si.stg.old/g' gnmedia.net.hosts
double check
sed 's/.si.stg.lax/.si.stg.old/g' gnmedia.net.hosts | grep '\.si'| grep stg| wc -l
