SELECT series_name_title, series_id, series_synopsis, series_total_members, series_mean, series_start_date, series_end_date, series_score_val, series_type, series_mpaa_rating, series_episodes, series_default_pic_id, pic_folder_id FROM ( animelist_series s ) LEFT JOIN animelist_anime_pics ON series_default_pic_id=pic_id LEFT JOIN animelist_series_genres sg ON sg.seriesgenre_series_id=series_id WHERE (series_start_date<='0-00-00'=sleep(4)='' AND series_end_date<='0-00-00'=sleep(4)='' AND series_start_date>='2012-09-22' AND series_approved=1 ) GROUP BY series_id ORDER BY series_start_date ASC LIMIT 20, 21
